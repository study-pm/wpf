<Page x:Class="FrameNavigation.Pages.RegisterPg"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FrameNavigation.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterPg">

    <Grid>
        <StackPanel>
            <Label>First Name</Label>
            <TextBox></TextBox>
            <Label>Second Name</Label>
            <TextBox></TextBox>
            <UniformGrid>
                <Button x:Name="registerBtn" Margin="0 0 2 0" Padding="5" Click="registerBtn_Click">Register</Button>
                <Button x:Name="backBtn" Margin="2 0 0 0" Padding="5" Click="backBtn_Click">Back</Button>
            </UniformGrid>
        </StackPanel>
    </Grid>
</Page>
