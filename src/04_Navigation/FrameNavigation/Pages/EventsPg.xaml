<Page x:Class="FrameNavigation.Pages.EventsPg"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:FrameNavigation.Pages"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      WindowTitle="Events"
      Title="EventsPg">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="backBtn" Click="backBtn_Click">Back (Unsafe)</Button>
        <Button x:Name="forwardBtn" Grid.Column="1" Click="forwardBtn_Click">Forward (Unsafe)</Button>
        <Button x:Name="backSafeBtn" Grid.Row="1" Click="backSafeBtn_Click">Back (Safe)</Button>
        <Button x:Name="forwardSafeBtn" Grid.Row="1" Grid.Column="1" Click="forwardSafeBtn_Click">Forward (Safe)</Button>
    </Grid>
</Page>
